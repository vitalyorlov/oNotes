{
    "manifest_version": 2,
 
    "name": "oNotes Plugin",
    "description": "This extension let you keep your notes simply and fast",
    "version": "1.0",
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab"
    ]
}